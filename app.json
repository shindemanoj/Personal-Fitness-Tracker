{
  "expo": {
    "name": "Personal Fitness Tracker",
    "slug": "personal-fitness-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "personal-fitness-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manoj.personalfitnesstracker",
      "buildNumber": "1",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "youtube"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "We send workout reminders to help you stay consistent with your fitness goals.",
        "NSMicrophoneUsageDescription": "This app does not record audio. Microphone access is not used."
      }
    },
    "android": {
      "package": "com.manoj.personalfitnesstracker",
      "permissions": [
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "shindemanoj",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dfb64655-8c5a-41ca-9eda-124e5540ed78"
      }
    }
  }
}
